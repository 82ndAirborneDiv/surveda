{
  "parser": "babel-eslint",
  "plugins": [ "react" ],
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "ecmaFeatures": {
    "arrowFunctions": true,
    "blockBindings": true,
    "classes": true,
    "defaultParams": true,
    "destructuring": true,
    "forOf": true,
    "generators": true,
    "modules": true,
    "spread": true,
    "templateStrings": true,
    "jsx": true
  },
  "rules": {
    "arrow-spacing": [1],
    "consistent-return": [0],
    "indent": [2, 2],
    "react/jsx-indent-props": [2, 2],
    "react/jsx-indent": [2, 2],
    "key-spacing": [0],
    "quotes": [0],
    "new-cap": [0],
    "no-cond-assign": [1],
    "no-const-assign": [2],
    "no-dupe-args": [2],
    "no-dupe-class-members": [2],
    "no-dupe-keys": [2],
    "no-duplicate-case": [2],
    "no-duplicate-imports": [1],
    "no-ex-assign": [1],
    "no-extra-boolean-cast": [1],
    "no-extra-semi": [1],
    "no-fallthrough": [1],
    "no-func-assign": [2],
    "no-implicit-coercion": [1],
    "no-mixed-spaces-and-tabs": [1],
    "no-multi-spaces": [0],
    "no-param-reassign": [1],
    "no-shadow": [1],
    "no-tabs": [1],
    "no-trailing-spaces": [1],
    "no-unreachable": [1],
    "no-unused-vars": [1, { "argsIgnorePattern": "^_" }],
    "object-curly-spacing": [1, "always"],
    "no-var": [1],
    "prefer-const": [1],
    "radix": [1],
    "use-isnan": [1],
    "react/jsx-no-undef": 1,
    "react/jsx-uses-react": 1,
    "react/jsx-uses-vars": 1,
    "react/jsx-closing-bracket-location": 1
  }
}
